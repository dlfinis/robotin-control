{
  "name": "robotin-control",
  "version": "0.1.0",
  "description": "Sistema cognitivo contextualizado para gestión de proyectos técnicos",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "robotin": "dist/cli/index.js"
  },
  "scripts": {
    "dev": "bun run src/cli/index.ts",
    "build": "bun build src/cli/index.ts --outdir dist --target bun",
    "start": "bun dist/index.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --apply",
    "format": "biome format src/ --write"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.0.0",
    "@elysiajs/swagger": "^1.0.0",
    "@keyv/sqlite": "^3.6.7",
    "@lancedb/lancedb": "0.23.0",
    "@xenova/transformers": "^2.17.2",
    "apache-arrow": "^21.1.0",
    "elysia": "^1.0.0",
    "keyv": "^4.5.4",
    "openai": "^6.17.0",
    "pino": "^8.17.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/better-sqlite3": "^7.6.8",
    "@types/bun": "latest",
    "bun-types": "latest",
    "pino-pretty": "^13.1.3",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.3.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "knowledge-management",
    "rag",
    "ai",
    "cli",
    "documentation"
  ],
  "author": "Robotin Team",
  "license": "MIT",
  "private": true
}
